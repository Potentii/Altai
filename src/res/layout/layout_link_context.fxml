<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.effect.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import java.net.*?>
<?import javafx.scene.layout.*?>
<?import java.net.URL?>

<BorderPane id="root" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.controller.context.LinkContextController">
    <stylesheets>
        <URL value="@../style/style_context.css" />
        <URL value="@../style/style_link_context.css" />
    </stylesheets>

    <center>
      <AnchorPane>
         <children>
              <ListView fx:id="listView" focusTraversable="false" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
            <Button contentDisplay="CENTER" ellipsisString="" graphicTextGap="0.0" mnemonicParsing="false" styleClass="FAB" AnchorPane.bottomAnchor="32.0" AnchorPane.rightAnchor="32.0">
               <graphic>
                  <ImageView fitHeight="24.0" fitWidth="24.0" pickOnBounds="true" preserveRatio="true" styleClass="FAB_img" />
               </graphic>
            </Button>
         </children>
         <StackPane.margin>
            <Insets bottom="16.0" left="16.0" right="16.0" top="16.0" />
         </StackPane.margin>
      </AnchorPane>
    </center>

   <top>
      <ToolBar id="appBar" BorderPane.alignment="CENTER">
        <items>
          <Button id="menuBtn" fx:id="menuBtn" cache="true" ellipsisString="" graphicTextGap="0.0" mnemonicParsing="false" styleClass="appBarBtn" />
            <Label id="appBarTitle" focusTraversable="false" graphicTextGap="0.0" styleClass="appBarText" text="Links" />
        </items>
      </ToolBar>
   </top>

</BorderPane>
